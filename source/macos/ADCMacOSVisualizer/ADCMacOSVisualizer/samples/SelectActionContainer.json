{
  "$schema": "http://adaptivecards.io/schemas/adaptive-card.json",
  "type": "AdaptiveCard",
  "version": "1.0",
  "body": [
    {
      "type": "Container",
      "items": [
        {
          "type": "TextBlock",
          "text": "Cool link"
        }
      ],
      "selectAction": {
        "type": "Action.OpenUrl",
        "url": "https://elgoog.im/"
      }
    },
    {
      "type": "Container",
      "style": "emphasis",
      "items": [
        {
          "type": "TextBlock",
          "text": "Emphasis link"
        }
      ],
      "selectAction": {
        "type": "Action.OpenUrl",
        "url": "https://msn.com"
      }
    },
    {
      "type": "Container",
      "style": "default",
      "items": [
        {
          "type": "TextBlock",
          "text": "Submit action"
        }
      ],
      "selectAction": {
        "type": "Action.Submit",
        "title": "Submit action",
        "data": {
          "info": "My submit action data"
        }
      }
    }
  ]
}
